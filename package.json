{
  "name": "stellarpos",
  "version": "1.0.0",
  "description": "Stellar Point of sale Application using react and electron",
  "main": "./out/main/index.js",
  "author": "Josue Medina <devbyjosue@gmail.com>",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "npx @biomejs/biome format --write ./src/renderer/",
	"lint": "npx @biomejs/biome lint --write ./src/renderer/",
	"check": "npx @biomejs/biome check --write ./src/renderer/",
	"merge-dev": "npm run format && git add --all && git commit -m 'time_to_merge' && git checkout dev && git merge $(git rev-parse --abbrev-ref @{-1}) && git checkout -",
	"start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@material-tailwind/react": "^2.1.9",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@mui/x-charts": "^7.11.0",
    "react-router-dom": "^6.25.1",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1"
  }
}
